<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.retrofit">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.Semana16_retrofit" >
        <activity
            android:name="com.example.retrofit.VersionApp"
            android:exported="false" />
        <activity
            android:name="com.example.retrofit.Informacion"
            android:exported="false" />
        <activity
            android:name="com.example.retrofit.Integrantes"
            android:exported="false" />
        <activity
            android:name="com.example.retrofit.usersActivity"
            android:exported="false" />
        <activity
            android:name="com.example.retrofit.albumsActivity"
            android:exported="false" />
        <activity
            android:name="com.example.retrofit.commentsActivity"
            android:exported="false" />
        <activity
            android:name="com.example.retrofit.postActivity"
            android:exported="false" />
        <activity android:name="com.example.retrofit.MainActivity" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>